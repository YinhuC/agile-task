{
  "name": "agile-tasker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cd client && yarn start",
    "start:server": "cd server && yarn start:dev",
    "build": "cd client && yarn build",
    "test": "cd client && yarn test",
    "test:server": "cd server && yarn test",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "postinstall": "cd client && yarn install --ignore-optional && cd ../server && yarn install --ignore-optional && rm -rf ../node_modules"
  }
}
